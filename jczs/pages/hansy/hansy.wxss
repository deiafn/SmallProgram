.in_1{
  padding: 24rpx 26rpx;
  position: relative;
  border-bottom: 2rpx solid #f4f4f4;
}
.in_s{
  position: fixed;
  top: 0;
  background: #fff;
}
.in_1 image{
  position: absolute;
  top: 40rpx;
  left: 46rpx;
  width: 30rpx;
  height: 30rpx;
}
.in_1 input{
  height: 60rpx;
  border-radius: 50px;
  border: 0;
  background: #f4f4f4;
  font-size: 21rpx;
  padding-left: 60rpx;
}
.in_2{
  display: flex;
  height: 74rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.in_2 view{
  text-align: center;
  width: 150rpx;
  font-size: 25rpx;
  line-height: 72rpx;
}
.in_2 view:nth-of-type(4) image{
  width: 13rpx;
  height: 21rpx;
}
.active{
  color: red;
}
.in_2 view:nth-of-type(5) image{
  width: 30rpx;
  height: 30rpx;
}
.in_3{
  margin-top: 182rpx;
  background: #f1f1f1;
  padding: 10rpx 0;
  display: flex;
  flex-wrap: wrap;
}
.in_3>view{
  width: 370rpx;
  height: 562rpx;
  background: #fff;
  margin-bottom: 9rpx;
  margin-right: 10rpx;
}
.in_3>view:nth-of-type(even){
  margin-right: 0;
}
.in_3>view image{
  width: 302rpx;
  height: 302rpx;
  padding: 14rpx 34rpx;
}
.in_3 view view:nth-of-type(1){
  font-size: 24rpx;
  line-height: 38rpx;
  height: 76rpx;
  padding: 0 32rpx 0 16rpx;
  color: #000;
}
.in_3 view view{
  padding: 0 32rpx 0 16rpx;
  line-height: 38rpx;
  color: #a4a4a4;
  font-size: 21rpx;
}
.in_3 view text{
  font-size: 30rpx;
  margin-right: 28rpx;
  color: red;
}