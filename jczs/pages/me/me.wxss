.in_1{
  width: 100%;
  height: 250rpx;
  background: #7ed321;
}
.in_1 view image{
  width: 128rpx;
  height: 128rpx;
  border-radius: 50px;
}
.in_1 view{
  padding-top: 60rpx;
  padding-left: 28rpx;
  position: relative;
}
.in_1 view text:nth-of-type(1){
  position: absolute;
  top: 80rpx;
  left: 185rpx;
  font-size: 30rpx;
  color: #fff;
}
.in_1 view text:nth-of-type(2){
  position: absolute;
  top: 145rpx;
  left: 185rpx;
  font-size: 18rpx;
  text-decoration: underline;
  color: #fff;
}
.in_2{
  padding-top: 19rpx;
  padding-bottom: 12rpx;
  background: #f1f1f1;
}
.in_2>view{
  border-top: 2rpx solid #e7e7e7;
  border-bottom: 2rpx solid #e7e7e7;
}
.in_2>view:nth-of-type(1){
  border-bottom:0;
  font-size: 28rpx;
  line-height: 88rpx;
  background: #fff;
  text-indent: 32rpx;
}
.in_2>view:nth-of-type(1) text{
  font-size: 25rpx;
  float: right;
  margin-right: 12rpx;
}
.in_2>view:nth-of-type(1) image{
  float: right;
  width: 12rpx;
  height: 20rpx;
  margin-top: 34rpx;
  margin-right: 30rpx;
}
.in_3{
  display: flex;
  background: #fff;
  padding: 18rpx 0;
}
.in_3 view{
  width: 150rpx;
  text-align: center;
  font-size: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 5rpx;
}
.in_3 view image{
  width: 37rpx;
  height: 37rpx;
}
.in_4{
  display: flex;
  flex-wrap: wrap;
  margin-top: 1rpx solid #e7e7e7;
}
.in_4>view{
  text-align: center;
  width: 187.5rpx;
  font-size: 20rpx;
  margin-top: 48rpx;
}
.in_4 view view{
  line-height: 64rpx;
  margin-bottom: 27rpx;
}
.in_4 view image{
  width: 49rpx;
  height: 49rpx;
}